<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>DataSearch</servlet-name>
        <servlet-class>com.jitlogic.zorka.central.roof.RoofServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>DataSearch</servlet-name>
        <url-pattern>/roof/*</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>ZorkaCentral.html</welcome-file>
    </welcome-file-list>
</web-app>
